{
	"$schema": "https://docs.renovatebot.com/renovate-schema.json",
	"branchPrefix": "mewmba/renovate/",
	"gitAuthor": "mewmba Renovate Bot <mewmba@trinsic.id>",
	"repositories": ["trinsic-id/sdk"],
	"additionalBranchPrefix": "{{manager}}-",
	"groupName": "trinsic sdk {{manager}} dependencies",
	"groupSlug": "trinsic sdk",
	"assignees": ["lucasamonrc"],
	"reviewers": ["lucasamonrc"],
	"prHourlyLimit": 0,
	"ignorePaths": ["swift/**"],
	"packageRules": [
		{
			"matchPackagePatterns": ["*"],
			"enabled": false
		},
		{
			"matchPackageNames": [
				"Trinsic.Api",
				"github.com/trinsic-id/sdk-go-api",
				"com.github.trinsic-id:sdk-java-api",
				"trinsic/api",
				"Trinsic-Api",
				"trinsic_api",
				"@trinsic/api",
				"com.github.trinsic-id:sdk-android-ui",
				"@trinsic/expo-ui",
				"trinsic_flutter_ui",
				"@trinsic/react-native-ui",
				"trinsic-ui-ios",
				"@trinsic/web-ui"
			],
			"enabled": true
		}
	]
}
