{
    "name": "trinsic/service-results",
    "description": "ServiceResults project for standardized result handling in Trinsic SDK",
    "type": "library",
    "license": "MIT",
    "keywords": [
        "trinsic",
        "verification",
        "identity",
        "results",
        "sdk"
    ],
    "authors": [
        {
            "name": "Trinsic ID",
            "email": "support@trinsic.id"
        }
    ],
    "require": {
        "php": ">=8.0"
    },
    "require-dev": {
